<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="true"
            class="net.todd.biblestudy.reference.common.views.ReferenceView"
            icon="icons/docs.png"
            id="net.todd.biblestudy.reference.common.ReferenceView"
            name="ReferenceView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="net.todd.biblestudy.reference.common.actionSet1"
            label="Global Actions"
            visible="true">
         <action
               class="net.todd.biblestudy.reference.common.actions.OpenReferenceAction"
               icon="icons/docs.png"
               id="net.todd.biblestudy.reference.common.OpenReferenceAction"
               label="Open Reference"
               toolbarPath="main/global"
               tooltip="Open Reference">
         </action>
      </actionSet>
   </extension>

</plugin>
